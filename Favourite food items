#include <stdio.h>
int main()
 {
   int number;
   printf("\n enter a number from 1 to 5 :");
   scanf("%d",&number);
   switch(number)
   {
     case 1: printf("Food item-Pizza");
             printf("\nPrice-Rs 239");
             break;
     case 2: printf("Food item-Chocolate icecream");
             printf("\nPrice-Rs 129");
             break;
    case 3: printf("Food item-Pani Puri");
             printf("\nPrice-Rs 179");
             break;
    case 4: printf("Food item-French Fries");
             printf("\nPrice-Rs 99");
             break;
    case 5: printf("Food item-Chicken rolls");
             printf("\nPrice-Rs 149");
             break;                             
     
  }
  return 0;
 }
